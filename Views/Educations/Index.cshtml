            <th>
                @Html.DisplayNameFor(model => model.InstituteType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.YOP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Program)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Elective)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ClgName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BoardName)
