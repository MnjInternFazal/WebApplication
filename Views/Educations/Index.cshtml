                @Html.DisplayFor(modelItem => item.ScoreType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CGPAValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CGPAScale)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CGPAPercentage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalMarks)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaxMarks)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MarkPercentage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Personal.PersonalId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EducationId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EducationId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EducationId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
